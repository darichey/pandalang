{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "PandaLang",
  "patterns": [
    {
      "include": "#keywords"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.pandalang",
          "match": "\\b(fun|let|in)\\b"
        }
      ]
    }
  },
  "scopeName": "source.panda"
}
